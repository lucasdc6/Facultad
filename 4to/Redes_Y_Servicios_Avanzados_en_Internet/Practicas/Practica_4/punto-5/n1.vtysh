!
!
service integrated-vtysh-config
!
interface eth0
 ip address 10.0.0.1/24
!
interface eth1
 ip address 10.0.1.1/24
!
interface gre0
!
interface gretap0
!
interface lo
!
router ospf
 ospf router-id 10.0.0.1
 redistribute static
 network 10.0.0.0/24 area 0.0.0.2
 network 10.0.1.0/24 area 0.0.0.0
 area 0.0.0.2 nssa translate-always
!
ip forwarding
ipv6 forwarding
!
line vty
!
end
