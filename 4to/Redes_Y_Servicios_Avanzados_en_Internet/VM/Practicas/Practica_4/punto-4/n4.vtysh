
!
!
service integrated-vtysh-config
!
interface eth0
 ip address 10.0.5.2/24
!
interface eth1
!
interface eth2
 ip address 10.0.6.1/24
!
interface lo
!
router ospf
 network 10.0.1.0/24 area 0.0.0.0
 network 10.0.5.0/24 area 0.0.0.0
 network 10.0.6.0/24 area 0.0.0.2
 area 0.0.0.2 stub
!
ip forwarding
ipv6 forwarding
!
line vty
!
end
