!
!
service integrated-vtysh-config
!
interface eth0
 ip address 10.0.0.2/24
 ipv6 nd suppress-ra
 no link-detect
!
interface eth1
 ip address 10.0.1.1/24
 ipv6 nd suppress-ra
 no link-detect
!
interface eth2
 ip address 10.0.5.2/24
 ipv6 nd suppress-ra
 no link-detect
!
interface lo
 no link-detect
!
router rip
 network 10.0.0.0/24
 network 10.0.1.0/24
 network 10.0.5.0/24
!
ip forwarding
ipv6 forwarding
!
line vty
!
end
