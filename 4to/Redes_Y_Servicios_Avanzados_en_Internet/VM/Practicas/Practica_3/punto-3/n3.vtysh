!
!
service integrated-vtysh-config
!
interface eth0
 ip address 10.0.2.2/24
!
interface eth1
!
interface eth2
 ip address 10.0.4.1/24
!
interface lo
!
router rip
 network 10.0.2.0/24
 network 10.0.3.0/24
 network 10.0.4.0/24
!
router ospf
 network 10.0.2.0/24 area 0.0.0.0
 network 10.0.4.0/24 area 0.0.0.0
!
ip forwarding
ipv6 forwarding
!
line vty
!
end
