!
!
service integrated-vtysh-config
!
interface eth0
 ip address 10.0.1.1/24
 ipv6 nd suppress-ra
 no link-detect
!
interface eth1
 ip address 10.0.2.1/24
 ipv6 nd suppress-ra
 no link-detect
!
interface lo
 no link-detect
!
router rip
 network 10.0.1.0/24
 network 10.0.2.0/24
 network 10.0.2.0724
!
ip forwarding
ipv6 forwarding
!
line vty
!
end
