!
!
service integrated-vtysh-config
!
interface eth0
 no link-detect
 ip address 10.0.1.2/24
 ipv6 nd suppress-ra
!
interface eth1
 no link-detect
 ip address 10.0.2.1/24
 ipv6 nd suppress-ra
!
interface lo
 no link-detect
!
router ospf
 network 10.0.1.0/24 area 0.0.0.0
 network 10.0.2.0/24 area 0.0.0.0
!
ip forwarding
ipv6 forwarding
!
line vty
!
end
